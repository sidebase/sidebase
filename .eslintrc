{
    "extends": "@antfu/vue",
    "overrides": [
        {
            "files": [
                "./server/**/*.ts"
            ],
            "rules": {
                "no-console": "off",
                "no-undef": "error"
            }
        }
    ],
    "rules": {
        "curly": [
            "error",
            "all"
        ],
        "brace-style": "off",
        "@typescript-eslint/brace-style": [
            "error",
            "1tbs"
        ],
        "no-unused-vars": "error"
    }
}
